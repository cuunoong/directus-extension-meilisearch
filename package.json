{
    "name": "directus-extension-meilisearch",
    "description": "A Directus extension to integrate Meilisearch, enabling fast and flexible full-text search capabilities within Directus collections.",
    "icon": "extension",
    "version": "1.0.5",
    "keywords": [
        "directus",
        "directus-extension",
        "directus-extension-hook",
        "meilisearch",
        "full-text search",
        "search",
        "fast",
        "integration"
    ],
    "author": "Arif Iskandar",
    "license": "MIT",
    "type": "module",
    "files": [
        "dist"
    ],
    "directus:extension": {
        "type": "hook",
        "path": "dist/index.js",
        "source": "src/index.js",
        "host": "^10.10.0"
    },
    "scripts": {
        "build": "directus-extension build",
        "dev": "directus-extension build -w --no-minify",
        "link": "directus-extension link"
    },
    "devDependencies": {
        "@directus/extensions-sdk": "12.0.2"
    },
    "dependencies": {
        "meilisearch": "^0.42.0",
        "string-strip-html": "^13.4.8"
    }
}
